{
  "name": "victor-hugo",
  "version": "1.0.0",
  "description": "Victor Hugo is a Hugo boilerplate for creating truly epic websites!",
  "repository": "netlify/victor-hugo",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "format": "standardx --fix src/**/*.js",
    "start": "run-p start:**",
    "start:hugo": "hugo -d ../dist -D -s site -vw",
    "start:webpack": "webpack-dev-server --config webpack.dev.js --hot",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist",
    "build": "npm run build:webpack && npm run build:hugo && npm run build:images",
    "build:preview": "npm run build:webpack && npm run build:hugo:preview && npm run build:images",
    "build:hugo": "env HUGO_BASEURL=$DEPLOY_PRIME_URL hugo -d ../dist -s site -v",
    "build:hugo:preview": "env HUGO_BASEURL=$DEPLOY_PRIME_URL npm run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "build:images": "find ./dist -type f \\( -iname \\*.jpg -o -iname \\*.jpeg -o -iname \\*.png \\) -exec sh -c \"./node_modules/.bin/imagemin {} --plugin.webp.lossless=true > {}.webp\" \\;"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/plugin-syntax-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.12.1",
    "@babel/register": "7.12.1",
    "@tailwindcss/typography": "0.2.0",
    "@tailwindcss/ui": "0.6.2",
    "alpinejs": "2.2.5",
    "assets-webpack-plugin": "3.9.7",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.1.0",
    "biginteger": "1.0.3",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.3",
    "cross-env": "5.2.0",
    "css-loader": "3.5.3",
    "d3": "6.2.0",
    "d3-collection": "1.0.7",
    "d3-color": "2.0.0",
    "d3-scale": "3.2.3",
    "d3-selection": "2.0.0",
    "d3-time": "2.0.0",
    "d3-time-format": "3.0.0",
    "d3-transition": "2.0.0",
    "eslint": "6.7.2",
    "eslint-plugin-import": "2.22.1",
    "exports-loader": "0.7.0",
    "fancy-log": "1.3.2",
    "file-loader": "4.1.0",
    "hugo-bin": "0.64.5",
    "imagemin": "7.0.1",
    "imagemin-cli": "6.0.0",
    "imagemin-webp": "6.0.0",
    "imports-loader": "0.8.0",
    "mini-css-extract-plugin": "0.12.0",
    "node-sass": "4.13.1",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "plugin-error": "1.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-url": "8.0.0",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "standardx": "5.0.0",
    "style-loader": "1.0.0",
    "tailwindcss": "1.9.0",
    "terser-webpack-plugin": "2.3.5",
    "url-loader": "2.1.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.1.4",
    "whatwg-fetch": "3.0.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}